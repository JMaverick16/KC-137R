<!-- Landing and Search Light fake cone - disable in rembrand  -->
	<animation>
	<type>select</type>
		<object-name>Cone-texture</object-name>
		<condition>
			<and>
				<property>sim/multiplay/generic/int[0]</property>
				<property>gear/gear[0]/position-norm</property>
		          <greater-than>
		            <property>sim/multiplay/generic/float[8]</property>
		            <value>15</value>
		          </greater-than>	
			</and>
		</condition>
	</animation>
	
	<animation>
	<type>select</type>
  	<object-name>Cone-texture.001</object-name>
 		<object-name>Cone-texture.002</object-name>
		<condition>
			<and>
				<property>sim/multiplay/generic/int[1]</property>
		          <greater-than>
		            <property>sim/multiplay/generic/float[8]</property>
		            <value>15</value>
		          </greater-than>	
			</and>
		</condition>
	</animation>
	
	<animation>
	<type>select</type>
		<object-name>Spot</object-name>
		<condition>
			<and>
				<not>
					<property>/sim/rendering/rembrandt/enabled</property>
				</not>
				<property>sim/multiplay/generic/int[0]</property>
				<property>gear/gear[0]/position-norm</property>	     
				  <less-than>
		            <property>sim/multiplay/generic/float[8]</property>
		            <value>15</value>
		          </less-than>
		          <greater-than>
		            <property>sim/multiplay/generic/float[8]</property>
		            <value>0</value>
		          </greater-than>			
			</and>
		</condition>
	</animation>
	
	<animation>
	<type>select</type>
  	<object-name>Spot.001</object-name>
 	<object-name>Spot.002</object-name>
		<condition>
			<and>
				<not>
					<property>/sim/rendering/rembrandt/enabled</property>
				</not>
				<property>sim/multiplay/generic/int[1]</property>	          
				  <less-than>
		            <property>sim/multiplay/generic/float[8]</property>
		            <value>15</value>
		          </less-than>
		          <greater-than>
		            <property>sim/multiplay/generic/float[8]</property>
		            <value>0</value>
		          </greater-than>
			</and>
		</condition>
	</animation>

