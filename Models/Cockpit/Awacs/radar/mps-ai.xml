<!-- TCAS ANIMATIONS -->
	
	<!-- TANKER only for refueling_demo -->

	<!-- ta -->

		<animation>
			<type>select</type>
			<object-name>ta</object-name>
			<condition>
				<property>instrumentation/mptcas/ta[0]/show-awacs</property>
			</condition>
		</animation>

		<animation>
			<type>pick</type>
			<object-name>ta</object-name>
			<visible>true</visible>
			<action>
				<button>0</button>
				<repeatable>false</repeatable>			
				<binding>
					<command>nasal</command>
					<script>b707.show_ta_awacs_info(0)</script>
				</binding>
			</action>
		</animation>
	
		<animation>
			<type>translate</type>
			<object-name>ta</object-name>
			<property>instrumentation/mptcas/ta[0]/dis-xa</property>
			<factor>4</factor>
			<axis>
				<x>0</x>
				<y>1</y>
				<z>0</z>
			</axis>
		</animation>
		<animation>
			<type>translate</type>
			<object-name>ta</object-name>
			<property>instrumentation/mptcas/ta[0]/dis-ya</property>
			<factor>4</factor>
			<axis>
				<x>0</x>
				<y>0</y>
				<z>1</z>
			</axis>
		</animation>

	
