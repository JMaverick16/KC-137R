  <factor>0.1</factor>
  <step>1</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>HdgDigit1</object-name>
  <property>/autopilot/settings/heading-bug-deg</property>
