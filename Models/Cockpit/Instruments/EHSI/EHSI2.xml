<?xml version="1.0"?>
<PropertyList>

<path>ehsi.ac</path>

<animation>
  <type>material</type>
  <object-name>Ticks</object-name>
  <object-name>CompassRose</object-name>
  <object-name>VSI.needle</object-name>
  <object-name>LabelHdg</object-name>
  <object-name>LabelDME</object-name>
  <object-name>LabelCrs</object-name>
  <object-name>LabelGspd</object-name>
  <object-name>GspdDigit1</object-name>
  <object-name>GspdDigit2</object-name>
  <object-name>GspdDigit3</object-name>
  <object-name>CrsDigit1</object-name>
  <object-name>CrsDigit2</object-name>
  <object-name>CrsDigit3</object-name>
  <object-name>HdgDigit1</object-name>
  <object-name>HdgDigit2</object-name>
  <object-name>HdgDigit3</object-name>
  <object-name>CrsNeedle</object-name>
  <object-name>CrsNeedle1</object-name>
  <object-name>HdgBug</object-name>
  <object-name>Rudder.needle</object-name>
  <object-name>Rudder.scale</object-name>
  <object-name>ThrottleL</object-name>
  <object-name>ThrottleR</object-name>
  <object-name>Throttle.scale</object-name>
  <object-name>DMEDigit1</object-name>
  <object-name>DMEDigit2</object-name>
  <object-name>DMEDigit3</object-name>
  <emission>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
    <!--factor-prop>/controls/lighting/nd-norm</factor-prop-->
  </emission>
</animation>
<!--
<animation>
  <type>material</type>
  <object-name>Bezel2</object-name>
  <emission>
    <red>0.1</red>
    <green>0.05</green>
    <blue>0.025</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>
-->

<animation>
  <type>rotate</type>
  <object-name>HdgBug</object-name>
  <property>/autopilot/internal/fdm-heading-bug-error-deg</property>
  <axis>
    <x>-1.0</x>
    <y>0</y>
    <z>0.0</z>
  </axis>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.007</z-m>
  </center>
</animation>
<animation>
  <type>rotate</type>
  <object-name>CompassRose</object-name>
  <property>orientation/heading-magnetic-deg</property>
  <axis>
    <x>1.0</x>
    <y>0</y>
    <z>0.0</z>
  </axis>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.007</z-m>
  </center>
</animation>

<animation>
  <type>rotate</type>
  <object-name>CrsNeedle</object-name>
  <object-name>CrsNeedle1</object-name>
  <property>orientation/heading-magnetic-deg</property>
  <axis>
    <x>1.0</x>
    <y>0</y>
    <z>0.0</z>
  </axis>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.007</z-m>
  </center>
</animation>
<animation>
  <type>rotate</type>
  <object-name>CrsNeedle</object-name>
  <object-name>CrsNeedle1</object-name>
  <property>/instrumentation/nav[1]/radials/selected-deg</property>
  <axis>
    <x>-1.0</x>
    <y>0</y>
    <z>0.0</z>
  </axis>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.007</z-m>
  </center>
</animation>
<animation>
  <type>translate</type>
  <object-name>CrsNeedle1</object-name>
  <property>/instrumentation/nav[1]/heading-needle-deflection</property>
  <interpolation>
    <entry><ind>-999.0</ind><dep>-0.02</dep></entry>
    <entry><ind> -10.0</ind><dep>-0.02</dep></entry>
    <entry><ind>   0.0</ind><dep>  0.0</dep></entry>
    <entry><ind>  10.0</ind><dep> 0.02</dep></entry>
    <entry><ind> 999.0</ind><dep> 0.02</dep></entry>
  </interpolation>
  <axis>
    <x>0</x>
    <y>1.0</y>
    <z>0.0</z>
  </axis>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.007</z-m>
  </center>
</animation>

<animation>
  <type>rotate</type>
  <object-name>VSI.needle</object-name>
  <property>autopilot/internal/vert-speed-fpm</property>
  <interpolation>
    <entry><ind>-3000.0</ind><dep>-31.0</dep></entry>
    <entry><ind>-2000.0</ind><dep>-24.0</dep></entry>
    <entry><ind>-1000.0</ind><dep>-17.5</dep></entry>
    <entry><ind>    0.0</ind><dep>  0.0</dep></entry>
    <entry><ind> 1000.0</ind><dep> 17.5</dep></entry>
    <entry><ind> 2000.0</ind><dep> 24.0</dep></entry>
    <entry><ind> 3000.0</ind><dep> 31.0</dep></entry>
  </interpolation>
  <axis>
    <x>-1.0</x>
    <y>0</y>
    <z>0.0</z>
  </axis>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.104</y-m>
    <z-m>0.007</z-m>
  </center>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>CrsDigit1</object-name>
  <property>/instrumentation/nav[1]/radials/selected-deg</property>
  <factor>0.001</factor>
  <step>100</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>textranslate</type>
  <object-name>CrsDigit2</object-name>
  <property>/instrumentation/nav[1]/radials/selected-deg</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>textranslate</type>
  <object-name>CrsDigit3</object-name>
  <property>/instrumentation/nav[1]/radials/selected-deg</property>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>HdgDigit1</object-name>
  <property>/autopilot/settings/heading-bug-deg</property>
  <factor>0.001</factor>
  <step>100</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>textranslate</type>
  <object-name>HdgDigit2</object-name>
  <property>/autopilot/settings/heading-bug-deg</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>textranslate</type>
  <object-name>HdgDigit3</object-name>
  <property>/autopilot/settings/heading-bug-deg</property>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>GspdDigit1</object-name>
  <property>velocities/groundspeed-kt</property>
  <factor>0.001</factor>
  <step>100</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>textranslate</type>
  <object-name>GspdDigit2</object-name>
  <property>velocities/groundspeed-kt</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>textranslate</type>
  <object-name>GspdDigit3</object-name>
  <property>velocities/groundspeed-kt</property>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>select</type>
  <object-name>LabelDME</object-name>
  <object-name>DMEDigit1</object-name>
  <object-name>DMEDigit2</object-name>
  <object-name>DMEDigit3</object-name>
  <condition>
    <property>/instrumentation/nav[1]/in-range</property>
  </condition>
</animation>
<animation>
  <type>textranslate</type>
  <object-name>DMEDigit1</object-name>
  <property>instrumentation/nav[1]/nav-distance</property>
        <factor>0.000000539956803</factor>
         <step>185200</step>
      <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
<animation>
  <type>textranslate</type>
  <object-name>DMEDigit2</object-name>
  <property>instrumentation/nav[1]/nav-distance</property>
        <factor>0.00000539956803</factor>
         <step>18520</step>
      <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
<animation>
  <type>textranslate</type>
  <object-name>DMEDigit3</object-name>
  <property>instrumentation/nav[1]/nav-distance</property>
        <factor>0.0000539956803</factor>
         <step>1852</step>
      <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

<animation>
  <type>select</type>
  <object-name>Rudder.needle</object-name>
  <object-name>Rudder.scale</object-name>
  <object-name>ThrottleL</object-name>
  <object-name>ThrottleR</object-name>
  <object-name>Throttle.scale</object-name>
  <condition>
    <property>/sim/instrument-options/hsi/show-rudder</property>
  </condition>
</animation>
<animation>
  <type>translate</type>
  <object-name>Rudder.needle</object-name>
  <property>controls/flight/rudder</property>
  <interpolation>
    <entry><ind>-1</ind><dep>-0.0175</dep></entry>
    <entry><ind> 0</ind><dep>      0</dep></entry>
    <entry><ind> 1</ind><dep> 0.0175</dep></entry>
  </interpolation>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
  <center>
    <x-m>0</x-m>
    <y-m>0</y-m>
    <z-m>0</z-m>
  </center>
</animation>
<animation>
  <type>translate</type>
  <object-name>ThrottleL</object-name>
  <property>controls/engines/engine[0]/throttle</property>
  <interpolation>
    <entry><ind>0</ind><dep>   0</dep></entry>
    <entry><ind>1</ind><dep>0.04</dep></entry>
  </interpolation>
  <axis>
    <x>0</x>
    <y>0</y>
    <z>1</z>
  </axis>
  <center>
    <x-m>0</x-m>
    <y-m>0</y-m>
    <z-m>0</z-m>
  </center>
</animation>
<animation>
  <type>translate</type>
  <object-name>ThrottleR</object-name>
  <property>controls/engines/engine[1]/throttle</property>
  <interpolation>
    <entry><ind>0</ind><dep>   0</dep></entry>
    <entry><ind>1</ind><dep>0.04</dep></entry>
  </interpolation>
  <axis>
    <x>0</x>
    <y>0</y>
    <z>1</z>
  </axis>
  <center>
    <x-m>0</x-m>
    <y-m>0</y-m>
    <z-m>0</z-m>
  </center>
</animation>


</PropertyList>
