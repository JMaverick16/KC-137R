    <animation>
        <type>pick</type>
        <object-name>Btn.menu</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-cycle</command>
                <property>/instrumentation/efb/keypress</property>
				<value>MENU</value>
            </binding>
            <binding>
                <command>nasal</command>
                <script>controls.click(9);</script>
            </binding>			
        </action>
