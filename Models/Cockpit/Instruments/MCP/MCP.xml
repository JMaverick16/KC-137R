			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>Ap1</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>it-autoflight/input/ap1</property>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>Ap2</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>it-autoflight/input/ap2</property>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>Ils</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>it-autoflight/input/vert</property>
				<value>2</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>Flch</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>it-autoflight/input/vert</property>
				<value>4</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>AltHld</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>it-autoflight/input/vert</property>
				<value>0</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>Vs</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>it-autoflight/input/vert</property>
				<value>1</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>Fpa</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>it-autoflight/input/vert</property>
				<value>5</value>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>Fd2</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>it-autoflight/input/fd2</property>
			</binding>
		</action>
	</animation>
	
	<!-- Knobs -->
	<animation>
		<type>pick</type>
		<object-name>Spd.knob</object-name>
		<action>
			<button>0</button>
			<button>3</button>
			<repeatable>true</repeatable>
			<binding>
				<condition>
					<and>
						<not><property>devices/status/keyboard/shift</property></not>
						<not><property>it-autoflight/input/kts-mach</property></not>
					</and>
				</condition>
				<command>nasal</command>
				<script>setprop("/it-autoflight/input/kts", math.min(getprop("/it-autoflight/input/kts") + 1, getprop("/it-autoflight/settings/max-kts")));</script>
			</binding>
			<binding>
				<condition>
					<and>
						<property>devices/status/keyboard/shift</property>
						<not><property>it-autoflight/input/kts-mach</property></not>
					</and>
				</condition>
				<command>nasal</command>
				<script>setprop("/it-autoflight/input/kts", math.min(getprop("/it-autoflight/input/kts") + 10, getprop("/it-autoflight/settings/max-kts")));</script>
			</binding>
			<binding>
				<condition>
					<and>
						<not><property>devices/status/keyboard/shift</property></not>
						<property>it-autoflight/input/kts-mach</property>
					</and>
				</condition>
				<command>nasal</command>
				<script>setprop("/it-autoflight/input/mach", math.round(math.min(getprop("/it-autoflight/input/mach") + 0.001, getprop("/it-autoflight/settings/max-mach")), 0.001));</script>
			</binding>
			<binding>
				<condition>
					<and>
						<property>devices/status/keyboard/shift</property>
						<property>it-autoflight/input/kts-mach</property>
					</and>
				</condition>
				<command>nasal</command>
				<script>setprop("/it-autoflight/input/mach", math.round(math.min(getprop("/it-autoflight/input/mach") + 0.01, getprop("/it-autoflight/settings/max-mach")), 0.001));</script>
			</binding>
		</action>
		<action>
			<button>1</button>
			<button>4</button>
			<repeatable>true</repeatable>
			<binding>
				<condition>
					<and>
						<not><property>devices/status/keyboard/shift</property></not>
						<not><property>it-autoflight/input/kts-mach</property></not>
					</and>
				</condition>
				<command>nasal</command>
				<script>setprop("/it-autoflight/input/kts", math.max(getprop("/it-autoflight/input/kts") - 1, 100));</script>
			</binding>
			<binding>
				<condition>
					<and>
						<property>devices/status/keyboard/shift</property>
						<not><property>it-autoflight/input/kts-mach</property></not>
					</and>
				</condition>
				<command>nasal</command>
				<script>setprop("/it-autoflight/input/kts", math.max(getprop("/it-autoflight/input/kts") - 10, 100));</script>
			</binding>
			<binding>
				<condition>
					<and>
						<not><property>devices/status/keyboard/shift</property></not>
						<property>it-autoflight/input/kts-mach</property>
					</and>
				</condition>
				<command>nasal</command>
				<script>setprop("/it-autoflight/input/mach", math.round(math.max(getprop("/it-autoflight/input/mach") - 0.001, 0.5), 0.001));</script>
			</binding>
			<binding>
				<condition>
					<and>
						<property>devices/status/keyboard/shift</property>
						<property>it-autoflight/input/kts-mach</property>
					</and>
				</condition>
				<command>nasal</command>
				<script>setprop("/it-autoflight/input/mach", math.round(math.max(getprop("/it-autoflight/input/mach") - 0.01, 0.5), 0.001));</script>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>Hdg.knob</object-name>
		<action>
			<button>0</button>
			<button>3</button>
			<repeatable>true</repeatable>
			<binding>
				<condition>
					<not><property>devices/status/keyboard/shift</property></not>
				</condition>
				<command>property-adjust</command>
				<property>it-autoflight/input/hdg</property>
				<step>1</step>
				<min>1</min>
				<max>361</max>
				<wrap>true</wrap>
			</binding>
			<binding>
				<condition>
					<property>devices/status/keyboard/shift</property>
				</condition>
				<command>property-adjust</command>
				<property>it-autoflight/input/hdg</property>
				<step>10</step>
				<min>1</min>
				<max>361</max>
				<wrap>true</wrap>
			</binding>
		</action>
		<action>
			<button>1</button>
			<button>4</button>
			<repeatable>true</repeatable>
			<binding>
				<condition>
					<not><property>devices/status/keyboard/shift</property></not>
				</condition>
				<command>property-adjust</command>
				<property>it-autoflight/input/hdg</property>
				<step>-1</step>
				<min>1</min>
				<max>361</max>
				<wrap>true</wrap>
			</binding>
			<binding>
				<condition>
					<property>devices/status/keyboard/shift</property>
				</condition>
				<command>property-adjust</command>
				<property>it-autoflight/input/hdg</property>
				<step>-10</step>
				<min>1</min>
				<max>361</max>
				<wrap>true</wrap>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>Alt.knob</object-name>
		<action>
			<button>0</button>
			<button>3</button>
			<repeatable>true</repeatable>
			<binding>
				<condition>
					<not><property>devices/status/keyboard/shift</property></not>
				</condition>
				<command>property-adjust</command>
				<property>it-autoflight/input/alt</property>
				<step>100</step>
				<max>50000</max>
			</binding>
			<binding>
				<condition>
					<property>devices/status/keyboard/shift</property>
				</condition>
				<command>property-adjust</command>
				<property>it-autoflight/input/alt</property>
				<step>1000</step>
				<max>50000</max>
			</binding>
		</action>
		<action>
			<button>1</button>
			<button>4</button>
			<repeatable>true</repeatable>
			<binding>
				<condition>
					<not><property>devices/status/keyboard/shift</property></not>
				</condition>
				<command>property-adjust</command>
				<property>it-autoflight/input/alt</property>
				<step>-100</step>
				<min>0</min>
			</binding>
			<binding>
				<condition>
					<property>devices/status/keyboard/shift</property>
				</condition>
				<command>property-adjust</command>
				<property>it-autoflight/input/alt</property>
				<step>-1000</step>
				<min>0</min>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>Pitch.knob</object-name>
		<drag-direction>vertical</drag-direction>
		<increase>
			<binding>
				<condition>
					<equals>
						<property>it-autoflight/output/vert</property>
						<value>1</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>it-autoflight/input/vs</property>
				<step>-100</step>
				<min>-6000</min>
			</binding>
			<binding>
				<condition>
					<equals>
						<property>it-autoflight/output/vert</property>
						<value>5</value>
					</equals>
				</condition>
				<command>nasal</command>
				<script>setprop("/it-autoflight/input/fpa", math.round(math.max(getprop("/it-autoflight/input/fpa") - 0.1, -9.9), 0.1));</script>
			</binding>
			<binding>
				<condition>
					<and>
						<not-equals>
							<property>it-autoflight/output/vert</property>
							<value>1</value>
						</not-equals>
						<not-equals>
							<property>it-autoflight/output/vert</property>
							<value>5</value>
						</not-equals>
					</and>
				</condition>
				<command>property-assign</command>
				<property>it-autoflight/input/vert</property>
				<value>1</value>
			</binding>
		</increase>
		<decrease>
			<binding>
				<condition>
					<equals>
						<property>it-autoflight/output/vert</property>
						<value>1</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>it-autoflight/input/vs</property>
				<step>100</step>
				<max>6000</max>
			</binding>
			<binding>
				<condition>
					<equals>
						<property>it-autoflight/output/vert</property>
						<value>5</value>
					</equals>
				</condition>
				<command>nasal</command>
				<script>setprop("/it-autoflight/input/fpa", math.round(math.min(getprop("/it-autoflight/input/fpa") + 0.1, 9.9), 0.1));</script>
			</binding>
			<binding>
				<condition>
					<and>
						<not-equals>
							<property>it-autoflight/output/vert</property>
							<value>1</value>
						</not-equals>
						<not-equals>
							<property>it-autoflight/output/vert</property>
							<value>5</value>
						</not-equals>
					</and>
				</condition>
				<command>property-assign</command>
				<property>it-autoflight/input/vert</property>
				<value>1</value>
			</binding>
		</decrease>
	</animation>
	
	<!-- Display -->
	<animation>
		<type>select</type>
		<object-name>ApIas</object-name>
		<object-name>ApIas1</object-name>
		<object-name>ApIas10</object-name>
		<object-name>ApIas100</object-name>
		<condition>
			<equals>
				<property>it-autoflight/input/kts-mach</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>ApIas100</object-name>
		<property>it-autoflight/input/kts</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>ApIas10</object-name>
		<property>it-autoflight/input/kts</property>
		<factor>0.01</factor>
		<step>10</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>ApIas1</object-name>
		<property>it-autoflight/input/kts</property>
		<factor>0.1</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>

	<animation>
		<type>select</type>
		<object-name>ApMach</object-name>
		<object-name>ApMach.1</object-name>
		<object-name>ApMach.01</object-name>
		<object-name>ApMach.001</object-name>
		<condition>
			<equals>
				<property>it-autoflight/input/kts-mach</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>ApMach.1</object-name>
		<property>it-autoflight/input/mach-x1000</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>ApMach.01</object-name>
		<property>it-autoflight/input/mach-x1000</property>
		<factor>0.01</factor>
		<step>10</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>ApMach.001</object-name>
		<property>it-autoflight/input/mach-x1000</property>
		<factor>0.1</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>ApHdg100</object-name>
		<property>it-autoflight/input/hdg</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>ApHdg10</object-name>
		<property>it-autoflight/input/hdg</property>
		<factor>0.01</factor>
		<step>10</step>
