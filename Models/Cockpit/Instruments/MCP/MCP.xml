		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>it-autoflight/input/lat</property>
				<value>3</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>HdgSel</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>it-autoflight/input/lat</property>
				<value>0</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>Loc</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>it-autoflight/input/lat</property>
