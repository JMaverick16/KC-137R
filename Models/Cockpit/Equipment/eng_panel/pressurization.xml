		<interpolation>
		  <entry><ind> 0</ind><dep>   0</dep></entry>
		  <entry><ind> 1</ind><dep>  41</dep></entry>
		  <entry><ind> 2</ind><dep>  76</dep></entry>
		  <entry><ind> 3</ind><dep> 110</dep></entry>
		</interpolation>
		<axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
