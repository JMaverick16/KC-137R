	<!-- Cover -->
  <animation>
    <type>rotate</type>
    <object-name>cover</object-name>
    <property>/b707/air-conditioning/ram-air-cover</property>
    <interpolation> 
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-90.0</dep>
            </entry>		
        </interpolation>
    <axis>
     <x> 1</x>
     <y> 0</y>
     <z> 0</z>
    </axis>
    <center>
    <x-m> 0.0</x-m>
    <y-m>-0.001</y-m>
    <z-m>0.1661</z-m>
    </center>  
  </animation>

	<animation>
		  <type>pick</type>
		  <object-name>cover</object-name>
		  <action>
		    <button>0</button>
		    <repeatable>false</repeatable>
		    <binding>
		        <command>property-toggle</command>
    				<property>/b707/air-conditioning/ram-air-cover</property>
		    </binding>
		    <binding>
		      <command>property-toggle</command>
		      <property>/sim/sound/switch3</property>
		    </binding>
		  </action>
	</animation>

