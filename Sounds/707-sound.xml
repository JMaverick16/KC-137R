<PropertyList>

 <fx>
 
  <apoff>
   <name>apoff</name>
   <type>avionics</type>
   <path>/Aircraft/KC-137R/Sounds/apoff.wav</path>
   <condition>
	 <property>/it-autoflight/sound/apoff</property>
   </condition>
   <volume>
    <factor>10.0</factor>
   </volume>
  </apoff>
  
   <apon>
   <name>apon</name>
   <type>avionics</type>
   <path>/Aircraft/KC-137R/Sounds/apon.wav</path>
   <condition>
	<greater-than>
      <property>/it-autoflight/output/ap1</property>
        <value>0</value>
     </greater-than>
   </condition>
   <volume>
    <factor>10.0</factor>
   </volume>
  </apon>
  
  <apon>
   <name>apon2</name>
   <path>/Aircraft/KC-137R/Sounds/apon.wav</path>
   <type>avionics</type>
   <condition>
   	<greater-than>
      <property>/it-autoflight/output/ap2</property>
        <value>0</value>
      </greater-than>
   </condition>
   <volume>
    <factor>10.0</factor>
   </volume>
  </apon>
  
		<altAlert>
			<name>altAlert</name>
			<mode>once</mode>
			<type>avionics</type>
			<path>Aircraft/KC-137R/Sounds/altAlert.wav</path>
			<condition>
				<or>
					<and>
						<greater-than>
							<property>sim/sound/altalert</property>
							<value>-1000</value>
						</greater-than>
						<less-than>
							<property>sim/sound/altalert</property>
							<value>-200</value>
						</less-than>
					</and>
					<and>
						<less-than>
							<property>sim/sound/altalert</property>
							<value>1000</value>
						</less-than>
						<greater-than>
							<property>sim/sound/altalert</property>
							<value>200</value>
						</greater-than>
					</and>
				</or>
			</condition>
			<volume>
				<factor>2.0</factor>
			</volume>
		</altAlert>
  
  <tchdwn0>
   <name>tchdwn0</name>
   <path>/Aircraft/KC-137R/Sounds/tchdwn.wav</path>
   <condition>
     <property>/gear/gear[0]wow</property>
   </condition>
   <volume>
    <factor>35.0</factor>
   </volume>
  </tchdwn0>
  
  <tchdwn1>
   <name>tchdwn1</name>
   <path>/Aircraft/KC-137R/Sounds/tchdwn.wav</path>
   <condition>
     <property>/gear/gear[1]wow</property>
   </condition>
   <volume>
    <factor>40.0</factor>
   </volume>
  </tchdwn1>
  
  <tchdwn2>
   <name>tchdwn2</name>
   <path>/Aircraft/KC-137R/Sounds/tchdwn.wav</path>
   <condition>
     <property>/gear/gear[2]/wow</property>
   </condition>
   <volume>
    <factor>40.0</factor>
   </volume>
  </tchdwn2>

  <tchdwn3>
   <name>tchdwn3</name>
   <path>/Aircraft/KC-137R/Sounds/touch_IN.wav</path>
   <condition>
     <property>/gear/gear[2]/wow</property>
   </condition>
   <volume>
   	<property>sim/current-view/internal</property>
    <factor>18.0</factor>
   </volume>
  </tchdwn3>
		
<!-- Other stuff -->

	<rumble>
		<name>rumbleOnGround</name>
		<mode>looped</mode>
		<path>Aircraft/KC-137R/Sounds/rumblingOnGround.wav</path>
		<condition>			
			<or>
				<property>/sim/current-view/internal</property>
				<equals>
					<property>/sim/current-view/name</property>
					<value type="string">Passengers View</value>
				</equals>
			</or>
			<or>
				<property>/gear/gear[0]/wow</property>
				<property>/gear/gear[1]/wow</property>
				<property>/gear/gear[2]/wow</property>
			</or>
		</condition>
		<volume>
			<property>/velocities/groundspeed-kt</property>
			<factor>2.0</factor>
		</volume>
		<pitch>
			<property>/velocities/groundspeed-kt</property>
			<factor>0.001</factor>
			<offset>0.5</offset>
		</pitch>
	</rumble>
	
	<gear>
		<name>gear</name>
		<mode>in-transit</mode>
		<path>gear.wav</path>
		<property>/gear/gear[0]/position-norm</property>
		<volume>
			<factor>1.5</factor>
		</volume>
		<pitch>
			<internal>dt_play</internal>
			<offset>1.0</offset>
			<factor>-0.001</factor>
		</pitch>
	</gear>

	<gear>
		<name>gear-lock</name>
		<path>Sounds/gear-lck.wav</path>
		<condition>
			<equals>
				<property>/gear/gear[0]/position-norm</property>
				<value>0</value>
			</equals>
			<not><property>/b707/crashed</property></not>
		</condition>
	</gear>
	
  <crashed>
    <name>AirplaneCrashed</name>
    <mode>looped</mode>
    <path>Aircraft/KC-137R/Sounds/crash_ground.wav</path>
     <condition>			
     	<and>
				<greater-than>
					<property>/velocities/groundspeed-kt</property>
					<value>2</value>
				</greater-than>
				<property>/b707/crashed</property>
		   </and>
     </condition>
			<volume>
				<property>/velocities/groundspeed-kt</property>
				<factor>2.0</factor>
			</volume>
			<pitch>
				<property>/velocities/groundspeed-kt</property>
				<factor>0.001</factor>
				<offset>0.5</offset>
			</pitch>
    </crashed>
 
	<flap>
		<name>flaps</name>
		<mode>in-transit</mode>
		<path>Aircraft/KC-137R/Sounds/Flaps.wav</path>
		<property>/surface-positions/flap-pos-norm</property>
		<volume>
			<factor>0.49</factor>
		</volume>
	</flap>
  
	<speedbrake>
		<name>speedbrake</name>	
		<mode>in-transit</mode>
		<path>Aircraft/KC-137R/Sounds/speedbrake.wav</path>
		<property>/surface-positions/speedbrake-pos-norm</property>
		<volume>
			<factor>0.6</factor>
		</volume>
		<pitch>
			<property>/surface-positions/speedbrake-pos-norm</property>
			<factor>0.4</factor>
		</pitch>
	</speedbrake>

	<start>
		<name>Start</name>	
		<mode>once</mode>
		<path>Aircraft/KC-137R/Sounds/turbine_start.wav</path>
		<condition>			
			<greater-than>
                    <property>engines/engine/starter</property>
                    <value>0</value>
                </greater-than>
		</condition>
		<volume>
			<factor>0.245</factor>
		</volume>
	</start>

	<start1>
		<name>Start1</name>	
		<mode>once</mode>
		<path>Aircraft/KC-137R/Sounds/turbine_start.wav</path>
		<condition>			
			<greater-than>
                    <property>engines/engine[1]/starter</property>
                    <value>0</value>
                </greater-than>
		</condition>
		<volume>
			<factor>0.245</factor>
		</volume>
	</start1>

	<start2>
		<name>Start2</name>	
		<mode>once</mode>
		<path>Aircraft/KC-137R/Sounds/turbine_start.wav</path>
		<condition>			
			<greater-than>
                    <property>engines/engine[2]/starter</property>
                    <value>0</value>
                </greater-than>
		</condition>
		<volume>
			<factor>0.245</factor>
		</volume>
	</start2>

	<start3>
		<name>Start3</name>	
		<mode>once</mode>
		<path>Aircraft/KC-137R/Sounds/turbine_start.wav</path>
		<condition>			
			<greater-than>
                    <property>engines/engine[3]/starter</property>
                    <value>0</value>
                </greater-than>
		</condition>
		<volume>
			<factor>0.245</factor>
		</volume>
	</start3>
	
	<reverser>
		<name>Reverse</name>	
		<mode>looped</mode>
		<path>Aircraft/KC-137R/Sounds/reverser.wav</path>
		<condition>			
			<or>
				<property>/engines/engine[0]/running</property>
				<property>/engines/engine[1]/running</property>
				<property>/engines/engine[2]/running</property>
				<property>/engines/engine[3]/running</property>     
			</or>
			<property>/engines/engine[0]/reversed</property>
		</condition>
		<volume>
			<property>/controls/engines/engine[0]/throttle</property>
			<factor>1</factor>
		</volume>
	</reverser>

	<wind>
		<name>wind</name>
		<mode>looped</mode>
		<path>wind.wav</path>
		<property>/velocities/airspeed-kt</property>
		<volume>
			<property>/position/altitude-ft</property>
			<factor>-0.000015</factor>
			<offset>1.0</offset>
			<min>0.1</min>
			<max>1.0</max>
		</volume>
		<volume>
			<property>/velocities/airspeed-kt</property>
			<factor>0.0015</factor>
			<min>0.03</min>
			<max>0.25</max>
		</volume>
		<pitch>
			<property>/velocities/airspeed-kt</property>
			<factor>0.01</factor>
			<offset>1.25</offset>
		</pitch>
	</wind>

	<stall>
        <name>stall</name>
        <mode>looped</mode>
        <path>Aircraft/KC-137R/Sounds/stall.wav</path>
        <condition>
			<greater-than>
				<property>/position/altitude-agl-ft</property>
				<value>200</value>
			</greater-than>
			<less-than>
				<property>/velocities/airspeed-kt</property>
				<value>125</value>
			</less-than>
        </condition>
        <volume>
            <offset>0.5</offset>
        </volume>
        <position>
            <x>-24</x>
            <y>0</y>
            <z>0.18</z>
        </position>
        <reference-dist>0.5</reference-dist>
        <max-dist>1.0</max-dist>
    </stall>

<overspeed>
        <name>Overspeed warning</name>
        <path>Aircraft/KC-137R/Sounds/speed.wav</path>
        <mode>looped</mode>
        <condition>
            <greater-than>
                <property>velocities/airspeed-kt</property>
                <value>380</value>
            </greater-than>
        </condition>
        <volume>
        	<property>sim/current-view/internal</property>
            <factor>1.0</factor>
        </volume>
    </overspeed>

	<testwarning>
		<name>test warning</name>
		<mode>looped</mode>
		<path>Aircraft/KC-137R/Sounds/warning.wav</path>
      <condition>
        <and>
					<greater-than>
						<property>/b707/ess-bus</property>
						<value>0</value>
					</greater-than>
					<or>
						<and>
							<greater-than>
								<property>/b707/pressurization/cabin-altitude</property>
								<value>8500</value>
							</greater-than>
							<not><property>/b707/pressurization/alt-cutout-horn</property></not>
						</and>
						<and>
							<greater-than>
								<property>/b707/pressurization/cabin-altitude</property>
								<value>4000</value>
							</greater-than>
							<not><property>/b707/pressurization/safety-valve</property></not>
							<not><property>/b707/pressurization/alt-cutout-horn</property></not>
						</and>
						<property>/controls/test/warn-knob</property>
					</or>
				</and>
      </condition>
		<volume>
			<property>sim/current-view/internal</property>
			<factor>1</factor>
		</volume>
	</testwarning>

	<firewarning>
		<name>fire warning</name>
		<mode>looped</mode>
		<path>Aircraft/KC-137R/Sounds/fire.wav</path>
      <condition>
      	<and>
					<greater-than>
						<property>/b707/ess-bus</property>
						<value>0</value>
					</greater-than>
		    	<or>
						<property>/controls/test/warn-knob</property>
						<property>/controls/engines/engine[0]/fire</property>
						<property>/controls/engines/engine[1]/fire</property>
						<property>/controls/engines/engine[2]/fire</property>
						<property>/controls/engines/engine[3]/fire</property>
					</or>
				</and>
      </condition>
		<volume>
			<property>sim/current-view/internal</property>
			<factor>1</factor>
		</volume>
	</firewarning>
	
	<icewarning>
		<name>ice warning</name>
		<mode>once</mode>
		<path>Aircraft/KC-137R/Sounds/warning.wav</path>
      <condition>
        <and>
					<greater-than>
						<property>/b707/ess-bus</property>
						<value>0</value>
					</greater-than>
				<property>/b707/warning/ice</property>
				<not><property>/b707/pressurization/alt-cutout-horn</property></not>
		</and>
      </condition>
		<volume>
			<property>sim/current-view/internal</property>
			<factor>1</factor>
		</volume>
	</icewarning>
		
	<call>
		<name>nosmoking</name>
		<mode>once</mode>
		<path>Aircraft/KC-137R/Sounds/cabinalert.wav</path>
      <condition>
        <and>
					<greater-than>
						<property>/b707/ess-bus</property>
						<value>0</value>
					</greater-than>
				 	<property>/b707/call/no-smoking</property>
				</and>
      </condition>
		<volume>
			<factor>1</factor>
		</volume>
	</call>
	
	<call>
		<name>seatbelts</name>
		<mode>once</mode>
		<path>Aircraft/KC-137R/Sounds/cabinalert.wav</path>
      <condition>
        <and>
					<greater-than>
						<property>/b707/ess-bus</property>
						<value>0</value>
					</greater-than>
				  <property>/b707/call/seat-belts</property>
				</and>
      </condition>
		<volume>
			<factor>1</factor>
		</volume>
	</call>
		
	<call>
		<name>grdcall</name>
		<mode>once</mode>
		<path>Aircraft/KC-137R/Sounds/groundcall.wav</path>
      <condition>
        <and>
					<greater-than>
						<property>/b707/ess-bus</property>
						<value>0</value>
					</greater-than>
					<property>/b707/call/crew-call</property>
				</and>
      </condition>
		<volume>
			<factor>1</factor>
		</volume>
	</call>
		
	<call>
		<name>cabinAtten</name>
		<mode>once</mode>
		<path>Aircraft/KC-137R/Sounds/groundcall.wav</path>
      <condition>
        <and>
					<greater-than>
						<property>/b707/ess-bus</property>
						<value>0</value>
					</greater-than>
					<property>/b707/call/cabin-attend</property>
				</and>
      </condition>
		<volume>
			<factor>3</factor>
		</volume>
	</call>
		
	<call>
		<name>persAccom</name>
		<mode>once</mode>
		<path>Aircraft/KC-137R/Sounds/groundcall.wav</path>
      <condition>
        <and>
					<greater-than>
						<property>/b707/ess-bus</property>
						<value>0</value>
					</greater-than>
					<property>/b707/call/pers-accom</property>
				</and>
      </condition>
		<volume>
			<factor>3</factor>
		</volume>
	</call>

    <rain>
			<name>RainOutside</name>
			<mode>looped</mode>
			<path>Aircraft/KC-137R/Sounds/rain.wav</path>
			<condition>
				<not>
					<property>sim/current-view/internal</property>
				</not>
				<greater-than>
					<property>/environment/rain-norm</property>
					<value>0</value>
				</greater-than>
			</condition>
			<volume>
				<property>/environment/rain-norm</property>
				<factor>0.41</factor>
			</volume>
		</rain>

		<rain>
			<name>RainInside</name>
			<mode>looped</mode>
			<path>Aircraft/KC-137R/Sounds/rain.wav</path>
			<condition>
				<property>sim/current-view/internal</property>
				<greater-than>
					<property>/environment/rain-norm</property>
					<value>0</value>
				</greater-than>
			</condition>
			<volume>
				<property>/environment/rain-norm</property>
				<factor>0.1</factor>
			</volume>
		</rain>

    <rain-windshield>
			<name>RainWindshieldInside</name>
			<mode>looped</mode>
			<path>Aircraft/KC-137R/Sounds/rain-windshield.wav</path>
			<condition>
				<property>sim/current-view/internal</property>
				<greater-than>
					<property>/environment/rain-norm</property>
					<value>0</value>
				</greater-than>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.008</factor>
				<min>0.04</min>
				<max>0.45</max>
			</volume>
		</rain-windshield>

	<pneumatic>
		<name>pneumatic break close</name>
		<mode>once</mode>
		<path>Aircraft/KC-137R/Sounds/slide.wav</path>
      <condition>
				 <property>/controls/gear/brake-parking</property>
      </condition>
		<volume>
			<factor>1</factor>
		</volume>
	</pneumatic>

	<pneumatic>
		<name>pneumatic break open</name>
		<mode>once</mode>
		<path>Aircraft/KC-137R/Sounds/slide.wav</path>
      <condition>
				 <not><property>/controls/gear/brake-parking</property></not>
      </condition>
		<volume>
			<factor>1</factor>
		</volume>
	</pneumatic>		
	
  <door>
