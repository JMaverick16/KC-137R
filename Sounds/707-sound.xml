	<probeLeft>
		<name>ProbeLeft</name>
		<mode>in-transit</mode>
		<path>Aircraft/KC-137R/Sounds/cargo_door.wav</path>
		<property>/b707/refuelling/probe-left-lever</property>
		<pitch>
			<internal>dt_play</internal>
			<offset>1.0</offset>
			<factor>-0.002</factor>
		</pitch>
