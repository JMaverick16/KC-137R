	<probeRight>
		<name>ProbeRight</name>
		<mode>in-transit</mode>
		<path>Aircraft/KC-137R/Sounds/cargo_door.wav</path>
		<property>/b707/refuelling/probe-right-lever</property>
		<pitch>
			<internal>dt_play</internal>
			<offset>1.0</offset>
			<factor>-0.002</factor>
		</pitch>
	</probeRight>
  
   <click>
   <name>click on</name>
   <path>Aircraft/KC-137R/Sounds/click.wav</path>
   <condition>
    <property>/instrumentation/generic/click</property>
   </condition>
   <volume>
    <max>0.45</max>
   </volume>
  </click>

