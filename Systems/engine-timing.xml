					</pow>
					<entry><ind>      18.0</ind><dep>0.00</dep></entry>
					<entry><ind>      22.4</ind><dep>0.03</dep></entry>
					<entry><ind>11394381.0</ind><dep>1.00</dep></entry>
				</table>
			</expression>
			<scale>/engines/epr-scale</scale>
		</input>
		<output>/engines/engine[0]/epr-actual-cmd</output>
	</filter>
	
