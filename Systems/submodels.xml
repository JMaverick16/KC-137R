<?xml version="1.0"?>

<PropertyList>

    <submodel>
        <name>flares1</name>
        <model>Aircraft/KC-137R/Models/Effects/flare.xml</model>
        <trigger>ai/submodels/submodel[0]/flare-release</trigger>        <!-- ai/submodels/submodel[0]/flare-release -->
        <speed>700.0</speed>        <!-- ft/sec -->
        <repeat>true</repeat>
        <delay>0.50</delay>
        <count>120</count>        <!-- -1 to have 'em all -->
        <x-offset>29.55</x-offset><!-- in feet -->
        <y-offset>0</y-offset>
        <z-offset>-6.55</z-offset>
        <yaw-offset>90.0</yaw-offset>
        <pitch-offset>-10.0</pitch-offset>
        <life>5</life>
        <buoyancy>0</buoyancy>
        <weight>2.5</weight>
        <eda>1.0</eda>
        <cd>1.0</cd>
        <no-roll>true</no-roll>
        <wind>true</wind>
    </submodel>

