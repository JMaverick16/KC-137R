<?xml version="1.0"?>

<!-- IT Autoflight: Autothrust -->
<!-- Copyright (c) 2025 Josh Davidson (Octal450) -->

<PropertyList>
	
	<logic>
		<input>
			<and>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>1</value>
				</equals>
				<equals>
					<property>/it-autoflight/input/athr-servo-clamp</property>
					<value>0</value>
				</equals>
			</and>
		</input>
		<output>/it-autoflight/output/athr-on</output>
	</logic>
	
	<filter>
		<name>A/THR Knots Lag</name>
		<type>exponential</type>
		<input>/it-autoflight/internal/kts-input-avg</input>
		<output>/it-autoflight/internal/athr-kts-input-lag</output>
		<filter-time>0.64</filter-time>
	</filter>
	
	<filter>
		<name>A/THR Knots Rate</name>
		<type>derivative</type>
		<input>/it-autoflight/internal/athr-kts-input-lag</input>
		<output>/it-autoflight/internal/athr-kts-rate-raw</output>
		<min>-10</min>
		<max>10</max>
		<filter-time>1.0</filter-time>
	</filter>
	
	<filter>
		<name>A/THR Mach Lag</name>
		<type>exponential</type>
		<input>/it-autoflight/internal/mach-input-avg</input>
		<output>/it-autoflight/internal/athr-mach-input-lag</output>
		<filter-time>0.64</filter-time>
	</filter>
	
	<filter>
		<name>A/THR Mach Rate</name>
		<type>derivative</type>
		<input>/it-autoflight/internal/athr-mach-input-lag</input>
		<output>/it-autoflight/internal/athr-mach-rate-raw</output>
		<filter-time>1.0</filter-time>
	</filter>
	
	<filter>
		<name>A/THR Mach Rate Gain</name>
		<type>gain</type>
		<gain>700</gain>
		<input>/it-autoflight/internal/athr-mach-rate-raw</input>
		<output>/it-autoflight/internal/athr-mach-rate-gain</output>
		<min>-10</min>
		<max>10</max>
	</filter>
	
	<filter>
		<name>A/THR Rate Filter</name>
		<type>exponential</type>
		<input>
			<condition>
				<less-than>
					<property>/velocities/airspeed-kt</property>
					<value>1</value>
				</less-than>
			</condition>
			<value>0</value>
		</input>
		<input>
			<condition>
				<equals>
					<property>/it-autoflight/input/kts-mach</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/it-autoflight/internal/athr-mach-rate-gain</property>
		</input>
		<input>/it-autoflight/internal/athr-kts-rate-raw</input>
		<output>/it-autoflight/internal/athr-rate</output>
		<filter-time>0.04</filter-time>
	</filter>
	
	<filter>
		<name>A/THR Knots Input Filter</name>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>/it-autoflight/input/kts</input>
		<output>/it-autoflight/internal/athr-kts</output>
		<max-rate-of-change>
			<condition>
				<and>
					<property>/it-autoflight/output/athr-on</property>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/it-autoflight/input/kts-mach</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<value>10</value>
		</max-rate-of-change>
		<max-rate-of-change>200</max-rate-of-change>
	</filter>
	
	<filter>
		<name>A/THR Mach Input Filter</name>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>/it-autoflight/input/mach</input>
		<output>/it-autoflight/internal/athr-mach</output>
		<max-rate-of-change>
			<condition>
				<and>
					<property>/it-autoflight/output/athr-on</property>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/it-autoflight/input/kts-mach</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<value>0.1</value>
		</max-rate-of-change>
		<max-rate-of-change>2</max-rate-of-change>
	</filter>
	
	<filter>
		<name>A/THR Rate Command</name>
		<type>gain</type>
		<gain>
			<expression>
				<table> <!-- As thrust decreases with altitude, correction must be less aggressive -->
					<property>/position/altitude-ft</property>
					<entry><ind>    0</ind><dep>1.0</dep></entry>
					<entry><ind>50000</ind><dep>0.7</dep></entry>
				</table>
			</expression>
		</gain>
		<input>
			<condition>
				<equals>
					<property>/it-autoflight/input/kts-mach</property>
					<value>1</value>
				</equals>
			</condition>
			<expression>
				<product>
					<difference>
						<property>/it-autoflight/internal/athr-mach</property>
						<property>/it-autoflight/internal/mach-asi-lag</property>
					</difference>
					<value>150</value>
				</product>
			</expression>
		</input>
		<input>
			<expression>
				<product>
					<difference>
						<property>/it-autoflight/internal/athr-kts</property>
						<property>/it-autoflight/internal/kts-asi-lag</property>
					</difference>
					<value>0.15</value>
				</product>
			</expression>
		</input>
		<output>/it-autoflight/internal/athr-rate-cmd-raw</output>
		<min>-10</min>
		<max>10</max>
	</filter>
	
	<filter>
		<name>A/THR Rate Command Filter</name>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>/it-autoflight/internal/athr-rate-cmd-raw</input>
		<output>/it-autoflight/internal/athr-rate-cmd</output>
		<max-rate-of-change>
			<condition>
				<and>
					<property>/it-autoflight/output/athr-on</property>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<value>3</value>
		</max-rate-of-change>
		<max-rate-of-change>100</max-rate-of-change>
	</filter>
	
	<filter>
		<name>A/THR Rate Error</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>/it-autoflight/internal/athr-rate</input>
		<reference>/it-autoflight/internal/athr-rate-cmd</reference>
		<output>/it-autoflight/internal/athr-rate-error</output>
	</filter>
	
	<pid-controller>
		<name>IT-CONTROLLER: Speed/Mach</name>
		<enable>
			<condition>
				<and>
					<property>/it-autoflight/output/athr-on</property>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>/it-autoflight/internal/athr-rate-error</input>
		<reference>0</reference>
		<output>/it-autoflight/internal/throttle-cmd-raw-pid</output>
		<config>
			<Kp>
				<expression>
					<table> <!-- As thrust decreases with altitude, more delta throttle is required -->
						<property>/position/altitude-ft</property>
						<entry><ind>    0</ind><dep>0.06</dep></entry>
						<entry><ind>50000</ind><dep>0.08</dep></entry>
					</table>
				</expression>
			</Kp>
			<Ti>1.2</Ti>
			<Td>0.0</Td>
			<u_min>
				<expression>
					<pow>
						<max>
							<property>/it-autoflight/settings/autothrottle-min</property>
							<sum>
								<property>/it-autoflight/internal/throttle-cmd</property>
								<value>-0.005</value> <!-- For 60Hz and 0.3/sec -->
							</sum>
						</max>
						<value>1.4</value>
					</pow>
				</expression>
			</u_min>
			<u_max>
				<expression>
					<pow>
						<min>
							<property>/it-autoflight/settings/autothrottle-max</property>
							<sum>
								<property>/it-autoflight/internal/throttle-cmd</property>
								<value>0.005</value> <!-- For 60Hz and 0.3/sec -->
							</sum>
						</min>
						<value>1.4</value>
					</pow>
				</expression>
			</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>Speed/Mach Shaping</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<property>/it-autoflight/output/athr-on</property>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<expression>
				<pow>
					<property>/it-autoflight/internal/throttle-cmd-raw-pid</property>
					<div>
						<value>1</value>
						<value>1.4</value>
					</div>
				</pow>
			</expression>
		</input>
		<output>/it-autoflight/internal/throttle-cmd-raw</output>
	</filter>
	
	<filter>
		<name>Speed/Mach Inactive Sync</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<or>
					<not><property>/it-autoflight/output/athr-on</property></not>
					<not-equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>0</value>
					</not-equals>
				</or>
			</condition>
		</enable>
		<input>
			<expression>
				<pow>
					<property>/it-autoflight/internal/throttle-cmd-raw</property>
					<value>1.4</value>
				</pow>
			</expression>
		</input>
		<output>/it-autoflight/internal/throttle-cmd-raw-pid</output>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: Idle Limit</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/athr-on</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<condition>
				<equals>
					<property>/it-autoflight/text/spd</property>
					<value>RETARD</value>
				</equals>
			</condition>
			<value>0</value>
		</input>
		<input>/it-autoflight/settings/autothrottle-min</input>
		<output>/it-autoflight/internal/throttle-cmd-raw</output>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: Thrust Limit</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/athr-on</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>2</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>/it-autoflight/settings/autothrottle-max</input>
		<output>/it-autoflight/internal/throttle-cmd-raw</output>
	</filter>
	
	<filter>
		<name>System Command: Throttle</name>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<enable>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr-on</property>
					<value>1</value>
				</equals>
			</condition>
		</enable>
		<input>/it-autoflight/internal/throttle-cmd-raw</input>
		<output>/it-autoflight/internal/throttle-cmd</output>
		<max-rate-of-change>
			<condition>
				<or>
					<and>
						<equals>
							<property>/it-autoflight/text/spd</property>
							<value>RETARD</value>
						</equals>
						<or>
							<equals>
								<property>/gear/gear[1]/wow</property>
								<value>1</value>
							</equals>
							<equals>
								<property>/gear/gear[2]/wow</property>
								<value>1</value>
							</equals>
						</or>
					</and>
					<equals> <!-- Don't slow throttle in T/O -->
						<property>/it-autoflight/output/vert</property>
						<value>7</value>
					</equals>
					<equals> <!-- Don't slow throttle in G/A -->
						<property>/it-autoflight/output/vert</property>
						<value>8</value>
					</equals>
				</or>
			</condition>
			<value>0.3</value>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<or>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>2</value>
					</equals>
				</or>
			</condition>
			<value>0.05</value>
		</max-rate-of-change>
		<max-rate-of-change>0.3</max-rate-of-change>
	</filter>
	
	<filter>
		<name>System Command: Throttle Comparator</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<max>
					<property>/controls/engines/engine[0]/throttle</property>
					<property>/controls/engines/engine[1]/throttle</property>
					<property>/controls/engines/engine[2]/throttle</property>
					<property>/controls/engines/engine[3]/throttle</property>
					<property>/controls/engines/engine[4]/throttle</property>
					<property>/controls/engines/engine[5]/throttle</property>
					<property>/controls/engines/engine[6]/throttle</property>
					<property>/controls/engines/engine[7]/throttle</property>
				</max>
			</expression>
		</input>
		<output>/it-autoflight/internal/throttle-compare-max</output>
	</filter>
	
	<filter>
		<name>System Command: Throttle Inactive Sync</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<not-equals>
					<property>/it-autoflight/output/athr-on</property>
					<value>1</value>
				</not-equals>
			</condition>
		</enable>
		<input>/it-autoflight/internal/throttle-compare-max</input>
		<output>/it-autoflight/internal/throttle-cmd</output>
	</filter>
	
	<!-- Engine 1 -->
	<filter>
		<name>System Command: Throttle 1 Filter</name>
		<type>noise-spike</type>
		<initialize-to>output</initialize-to>
		<input>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr-on</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/it-autoflight/internal/throttle-cmd</property>
		</input>
		<input>/controls/engines/engine[0]/throttle</input>
		<output>/it-autoflight/internal/throttle[0]</output>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr-on</property>
					<value>1</value>
				</equals>
			</condition>
			<value>0.3</value>
		</max-rate-of-change>
		<max-rate-of-change>100</max-rate-of-change>
	</filter>
	
	<filter>
		<name>System Command: Throttle Backdrive 1</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/athr-on</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/settings/use-controls-engines</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>/it-autoflight/internal/throttle[0]</input>
		<output>/controls/engines/engine[0]/throttle</output>
	</filter>
	
	<!-- Engine 2 -->
	<filter>
		<name>System Command: Throttle 2 Filter</name>
		<type>noise-spike</type>
		<initialize-to>output</initialize-to>
		<input>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr-on</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/it-autoflight/internal/throttle-cmd</property>
		</input>
		<input>/controls/engines/engine[1]/throttle</input>
		<output>/it-autoflight/internal/throttle[1]</output>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr-on</property>
					<value>1</value>
				</equals>
			</condition>
			<value>0.3</value>
		</max-rate-of-change>
		<max-rate-of-change>100</max-rate-of-change>
	</filter>
	
	<filter>
		<name>System Command: Throttle Backdrive 2</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/athr-on</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/settings/use-controls-engines</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>/it-autoflight/internal/throttle[1]</input>
		<output>/controls/engines/engine[1]/throttle</output>
	</filter>
	
	<!-- Engine 3 -->
	<filter>
		<name>System Command: Throttle 3 Filter</name>
		<type>noise-spike</type>
		<initialize-to>output</initialize-to>
		<input>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr-on</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/it-autoflight/internal/throttle-cmd</property>
		</input>
		<input>/controls/engines/engine[2]/throttle</input>
		<output>/it-autoflight/internal/throttle[2]</output>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr-on</property>
					<value>1</value>
				</equals>
			</condition>
			<value>0.3</value>
		</max-rate-of-change>
		<max-rate-of-change>100</max-rate-of-change>
	</filter>
	
	<filter>
		<name>System Command: Throttle Backdrive 3</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/athr-on</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/settings/use-controls-engines</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>/it-autoflight/internal/throttle[2]</input>
		<output>/controls/engines/engine[2]/throttle</output>
	</filter>
	
	<!-- Engine 4 -->
	<filter>
		<name>System Command: Throttle 4 Filter</name>
		<type>noise-spike</type>
		<initialize-to>output</initialize-to>
		<input>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr-on</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/it-autoflight/internal/throttle-cmd</property>
		</input>
		<input>/controls/engines/engine[3]/throttle</input>
		<output>/it-autoflight/internal/throttle[3]</output>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr-on</property>
					<value>1</value>
				</equals>
			</condition>
			<value>0.3</value>
		</max-rate-of-change>
		<max-rate-of-change>100</max-rate-of-change>
	</filter>
	
	<filter>
		<name>System Command: Throttle Backdrive 4</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/athr-on</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/settings/use-controls-engines</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>/it-autoflight/internal/throttle[3]</input>
		<output>/controls/engines/engine[3]/throttle</output>
	</filter>
	
